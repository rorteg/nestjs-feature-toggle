{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Debug Integration Tests",
      "runtimeExecutable": "npm",
      "runtimeArgs": [
        "run",
        "test:integration"
      ],
      "skipFiles": ["<node_internals>/**"],
      "console": "integratedTerminal",
      "env": {
        "APPLICATION_ENV": "test"
      },
      "sourceMaps": true
    }
  ]
}