{
  "name": "@rafaelortegabueno/nestjs-feature-toggle",
  "version": "2.0.0",
  "description": "Nest - modern, fast, powerful node.js web framework (@jwt)",
  "author": "Rafael Ortega Bueno",
  "license": "MIT",
  "scripts": {
    "format": "prettier --write \"**/*.ts\"",
    "lint": "eslint \"src/**/*.ts\" --fix",
    "test": "jest --config=jest.json",
    "test:watch": "jest --config=jest.json --watch",
    "test:coverage": "jest --config=jest.json --coverage --coverageDirectory=coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "build": "rm -rf dist && tsc -p tsconfig.build.json",
    "precommit": "lint-staged",
    "prepublish:npm": "npm run build",
    "publish:npm": "npm publish --access public",
    "prerelease": "npm run build",
    "release": "release-it",
    "start:dev": "nodemon  --exec ts-node --files e2e/src/main.ts",
    "commit": "cz",
    "test:e2e": "jest --config ./e2e/jest-e2e.json"
  },
  "peerDependencies": {
    "@nestjs/common": "^6.0.0 || ^7.0.0 || ^8.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^15.0.0",
    "@commitlint/config-angular": "^15.0.0",
    "@nestjs/common": "8.0.6",
    "@nestjs/core": "^8.0.6",
    "@nestjs/platform-express": "^8.0.6",
    "@nestjs/schematics": "^8.0.3",
    "@nestjs/testing": "^8.0.6",
    "@semantic-release/changelog": "^5.0.1",
    "@semantic-release/commit-analyzer": "^8.0.1",
    "@semantic-release/git": "^9.0.0",
    "@semantic-release/npm": "^7.0.6",
    "@semantic-release/release-notes-generator": "^9.0.1",
    "@types/jest": "26.0.24",
    "@types/node": "14.17.12",
    "@typescript-eslint/eslint-plugin": "4.29.3",
    "@typescript-eslint/parser": "4.29.3",
    "axios": "^0.21.1",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "7.32.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-import": "2.24.2",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "7.0.2",
    "jest": "27.1.0",
    "lint-staged": "11.1.2",
    "nodemon": "^2.0.12",
    "prettier": "2.3.2",
    "reflect-metadata": "0.1.13",
    "release-it": "14.14.3",
    "semantic-release": "^19.0.3",
    "ts-jest": "27.0.4",
    "typescript": "4.4.2"
  },
  "dependencies": {
    "@types/express": "^4.17.13",
    "@types/jsonwebtoken": "8.5.4",
    "express": "^4.17.1",
    "jsonwebtoken": "8.5.1"
  },
  "lint-staged": {
    "*.ts": [
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "prepare-commit-msg": "exec < /dev/tty && git cz --hook || true",
      "commit-msg": "commitlint -c .commitlintrc.json -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/rorteg/nestjs-feature-toggle"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
